#Peço um número e não transformo em inteiro para fazer as alterações  
x = input()

#Função que só inverte e retorno um inteiro
def inverter(x):
    #Caso o número seja negativo, ele diz
    if int(x) < 0:
        return 'Número negativo' 
    x = x[::-1]
    return int(x)

#Função que retona a quantidade de elementos
def contagem(x):
    if int(x) < 0:
        return 'Número negativo' 
    x = len(x)
    return x
